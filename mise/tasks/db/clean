#!/usr/bin/env bash
# mise description="clean the database"
# mise alias="cdb"
# mise dir="src/wac_lab"

DB_FILE="reflex.db"

if [ -f "$DB_FILE" ]; then
    echo "Removing $DB_FILE..."
    trash "$DB_FILE"
    echo "Database file removed successfully."
else
    echo "Database file $DB_FILE not found. Nothing to remove."
fi
